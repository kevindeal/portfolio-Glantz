{
  "name": "southleft",
  "version": "1.0.0",
  "description": "Southleft theme based on the Emulsify WP Theme",
  "author": "Southleft",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.6.3",
    "add-attributes-twig-extension": "^0.1.0",
    "autoprefixer": "^9.6.5",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-minify": "^0.5.0",
    "bem-twig-extension": "^0.1.1",
    "blueprint": "^2.1.5",
    "breakpoint-sass": "^2.7.1",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.0",
    "copy-webpack-plugin": "^6.3.2",
    "copyfiles": "^2.4.1",
    "cross-env": "6.0.3",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^4.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs": "^0.0.1-security",
    "glob": "^7.1.4",
    "imagemin-webpack-plugin": "^2.4.2",
    "jquery": "^3.5.1",
    "js-yaml-loader": "^1.2.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "node-sass-glob-importer": "^5.3.2",
    "normalize.css": "^8.0.1",
    "postcss-loader": "^3.0.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "sass-loader": "^8.0.0",
    "slick-carousel": "^1.8.1",
    "stylelint": "^12.0.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-webpack-plugin": "^1.0.2",
    "svg-sprite-loader": "^4.1.6",
    "twig": "~1.13.3",
    "twig-loader": "https://github.com/AmazeeLabs/twig-loader",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.2.2"
  },
  "resolutions": {
    "**/twig": "~1.13.3"
  },
  "scripts": {
    "setup": "yarn; npm install; composer install; yarn build;",
    "lint": "npm run -s lint:scripts && npm run -s lint:styles",
    "lint:scripts": "eslint ./assets/scripts ./patterns",
    "lint:styles": "stylelint \"patterns/**/*.{css,sass,scss,sss,less}\"",
    "storybook": "start-storybook -s ./dist, ./assets -p 6010",
    "build-storybook": "build-storybook -s dist -o storybook",
    "sb-build-static-assets": "run-s sb-copy-static-assets sb-clean-static-assets",
    "sb-copy-static-assets": "copyfiles -u 1 -V dist/images/* storybook/dist && copyfiles -u 1 -V dist/fonts/* storybook/dist && copyfiles -u 1 -V dist/js/* storybook/dist",
    "sb-clean-static-assets": "del storybook/images && del storybook/fonts && del storybook/js",
    "sb-clean-move": "rm -rf ../../../storybook; exec mv ./storybook ../../../.",
    "sb-publish": "run-s build-storybook sb-build-static-assets sb-clean-static-assets sb-clean-move",
    "webpack": "webpack -d --watch --config ./webpack/webpack.dev.js --display-error-details",
    "build-webpack": "webpack -p --config ./webpack/webpack.prod.js --display-error-details",
    "build": "npm run build-webpack",
    "develop": "concurrently --raw \"npm run webpack\" \"npm run storybook\"",
    "wp": "webpack -d --watch --config ./webpack/webpack.dev.js --display-error-details"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.1.9",
    "@storybook/addon-actions": "^6.1.9",
    "@storybook/addon-essentials": "^6.1.9",
    "@storybook/addon-links": "^6.1.9",
    "@storybook/react": "^6.1.9",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "core-js": "^3.8.0",
    "del-cli": "^3.0.1",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": ">= 10.12.0"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "no-empty-source": null,
      "max-empty-lines": 2,
      "length-zero-no-unit": null,
      "no-descending-specificity": null,
      "at-rule-empty-line-before": null,
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "extend",
            "at-root",
            "debug",
            "warn",
            "error",
            "if",
            "else",
            "for",
            "each",
            "while",
            "mixin",
            "include",
            "content",
            "return",
            "function"
          ]
        }
      ]
    }
  }
}
